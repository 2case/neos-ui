{
  "name": "@neos-project/neos-ui",
  "version": "1.0.0",
  "description": "Neos CMS UI written in ReactJS with Immutable data structures.",
  "main": "Resources/Public/JavaScript/Host.js",
  "private": true,
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "check-dependencies && npm run clean",
    "test": "npm run lint && nyc --lines 95 --functions 95 --branches 95 npm run ava",
    "test:watch": "npm run ava -- --watch",
    "build": "webpack --progress --colors",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf ./lib ./dist",
    "lint": "eslint src && stylelint 'src/**/*.css'",
    "ava": "ava",
    "precoverage:generate": "rimraf ./coverage",
    "coverage:generate": "nyc report --reporter=html --reporter=text-lcov > lcov.info",
    "postpublish": "greenkeeper-postpublish"
  },
  "ava": {
    "files": [
      "src/**/*.spec.js"
    ],
    "failFast": true,
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "exclude": [
      "src/**/*.spec.js",
      "src/**/*.story.js",
      "src/**/index.js"
    ],
    "include": [
      "src/**"
    ]
  },
  "devDependencies": {
    "@neos-project/eslint-config-neos": "^1.1.1",
    "ava": "^0.16.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "check-dependencies": "^1.0.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "lolex": "^1.5.1",
    "nyc": "^8.3.1",
    "object-assign": "^4.1.0",
    "postcss-css-variables": "^0.6.0",
    "postcss-hexrgba": "^0.2.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^1.0.0",
    "postcss-nested": "^1.0.0",
    "react-addons-shallow-compare": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "redux-devtools": "^3.2.0",
    "sinon": "^1.17.5",
    "skin-deep": "^0.16.0",
    "style-loader": "^0.13.0",
    "stylelint": "^7.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-livereload-plugin": "^0.9.0"
  },
  "dependencies": {
    "@neos-project/brand": "^1.1.0",
    "@neos-project/react-ui-components": "^4.3.0",
    "@reduct/registry": "^1.0.1",
    "classnames": "^2.2.3",
    "core-js": "^2.4.1",
    "debug": "^2.2.0",
    "immutable": "^3.8.0",
    "lodash.assign": "^4.2.0",
    "lodash.compose": "^2.4.1",
    "lodash.debounce": "^4.0.8",
    "lodash.defer": "^4.1.0",
    "lodash.isarray": "^4.0.0",
    "lodash.isfunction": "^3.0.8",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.memoize": "^4.1.2",
    "lodash.merge": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "mime-types": "^2.1.10",
    "moment": "^2.14.1",
    "monet": "^0.8.10",
    "normalize.css": "^5.0.0",
    "plow-js": "^1.2.0",
    "ramda": "^0.22.1",
    "react": "^15.3.1",
    "react-addons-shallow-compare": "^15.3.1",
    "react-click-outside": "^2.1.0",
    "react-collapse": "^2.3.3",
    "react-css-themr": "^1.2.0",
    "react-datetime": "^2.5.0",
    "react-dom": "^15.3.1",
    "react-dropzone": "^3.3.2",
    "react-height": "^2.0.4",
    "react-image-crop": "^1.0.0-rc2",
    "react-immutable-proptypes": "^2.0.0",
    "react-measure": "^1.2.0",
    "react-motion": "^0.4.2",
    "react-portal": "^2.2.1",
    "react-redux": "^4.4.5",
    "react-textarea-autosize": "^4.0.5",
    "redux": "^3.4.0",
    "redux-actions": "^0.12.0",
    "redux-saga": "^0.12.0",
    "regenerator": "^0.8.46",
    "reselect": "^2.4.0",
    "uuid": "^2.0.2",
    "whatwg-fetch": "^1.0.0"
  },
  "license": "GNU GPLv3"
}
